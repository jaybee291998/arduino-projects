void appendEnd(char c, uint8_t col_to_append){
  for(uint8_t col = 0; col < 7; col++){
    for(uint8_t row = 0; row < 8; row++){
      bufferArray[col + row * 8] = bufferArray[(col + 1) + row * 8];
    }
  }
  for(uint8_t row = 0; row < 8; row++){
    bufferArray[7 + row * 8] = characterMatrices[(uint8_t) c][col_to_append + row * 8];
  }
}

void printBuffer(uint32_t foregroundColor, uint32_t backgroundColor){
  for(int i = 0; i < NUM_LEDS; i++){
    leds[i] = bufferArray[i] == 1?foregroundColor:backgroundColor;
  }
  FastLED.show();
}

void scroll(String s){
  for(char c : s){
    for(uint8_t col = 0; col < WIDTH; col++){
      appendEnd(c, col);
      printBuffer(0xDAA520, 0x0000CD);
      delay(100);
    }
  }
}
