void setup_routing(){
  server.on("/post", HTTP_POST, handle_post);
  server.on("/stopCommand", HTTP_POST, handle_stop_scroll);

  server.begin();
}

void handle_post(){
  message = server.arg("message");
  int foregroundColor = server.arg("foreground-color").toInt();
  int backgroundColor = server.arg("background-color").toInt();
  int cycles = server.arg("cycles").toInt();
  int brightness = server.arg("brightness").toInt();
  int del = server.arg("delay").toInt();
  Serial.print("Message: ");
  Serial.println(message);
  Serial.print("Foreground: ");
  Serial.println(foregroundColor);
  Serial.print("Background: ");
  Serial.println(backgroundColor);
  Serial.print("Cycles: ");
  Serial.println(cycles);
  Serial.print("Brightness: ");
  Serial.println(brightness);
  Serial.print("Delay: ");
  Serial.println(del);
  server.send(200, "application/json", "{OK}");
  scroll(message, cycles, brightness, del, foregroundColor, backgroundColor);
}

void handle_stop_scroll(){
  String command = server.arg("command");
  if(!stopScroll && command == "stop") stopScroll = true;
  server.send(200, "application/json", "{OK}");
}

void setup() {
  FastLED.addLeds<LED_TYPE, DATA_PIN, COLOR_ORDER>(leds, NUM_LEDS).setCorrection(TypicalLEDStrip);
  FastLED.setBrightness(BRIGHTNESS);  
  // put your setup code here, to run once:
  Serial.begin(115200);

  Serial.print("Connecting to WiFi");
  WiFi.begin(SSID, PWD);
  uint8_t num_attempts = 0;
  while(WiFi.status() != WL_CONNECTED){
    Serial.print(".");
    delay(500);
    if(num_attempts >= MAX_ATTEMPTS){
      esp_sleep_enable_timer_wakeup(1 * uS_TO_S_FACTOR);
      esp_deep_sleep_start();
    }
    leds[num_attempts + 4 * WIDTH] = CRGB::Green;
    FastLED.show();
    num_attempts++;
  }
  fill_solid(leds, 64, CRGB::Red);
  FastLED.show();
  delay(1000);
  

  Serial.print("Connected! IP Address: ");
  Serial.println(WiFi.localIP());
  setup_routing();
  // put your setup code here, to run once:
  fill_solid(leds, 64, 0x000000);
  FastLED.show();
}

void loop() {
  server.handleClient();
}

void appendEnd(char c, uint8_t col_to_append){
  for(uint8_t col = 0; col < 7; col++){
    for(uint8_t row = 0; row < 8; row++){
      bufferArray[col + row * 8] = bufferArray[(col + 1) + row * 8];
    }
  }
  for(uint8_t row = 0; row < 8; row++){
    bufferArray[7 + row * 8] = characterMatrices[(uint8_t) c][col_to_append + row * 8];
  }
}

void printBuffer(uint32_t foregroundColor, uint32_t backgroundColor){
  for(int i = 0; i < NUM_LEDS; i++){
    leds[i] = bufferArray[i] == 1?foregroundColor:backgroundColor;
  }
  FastLED.show();
}

void scroll(String s, int cycles, uint8_t brightness, uint16_t del, uint32_t foreground, uint32_t background){
  FastLED.setBrightness(brightness);
  for(int cycle = 0; cycle < cycles; cycle++){
    for(char c : s){
      for(uint8_t col = 0; col < WIDTH; col++){
        server.handleClient();
        if(stopScroll){
          stopScroll = false;
          fill_solid(leds, 64, 0);
          return;
        }
        appendEnd(c, col);
        printBuffer(foreground, background);
        delay(del);
      }
    }
  }
  // turn all the leds off  
  fill_solid(leds, 64, 0x000000);
  FastLED.show();
}
